{
  "name": "bun-elysia-template",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile ./dist/server ./src/main.ts",
    "dev": "bun run --watch src/main.ts",
    "db.generate": "drizzle-kit generate",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.3.12",
    "@sinclair/typebox": "^0.34.41",
    "drizzle-orm": "^0.44.5",
    "drizzle-typebox": "^0.3.3",
    "elysia": "latest"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4",
    "husky": "^9.1.7"
  },
  "module": "src/index.js",
  "packageManager": "bun@1.2.8"
}
